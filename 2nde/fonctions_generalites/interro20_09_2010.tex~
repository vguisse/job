\begin{exercice}
 Soit $g$\baremeexo{4} la fonction définie sur $\R$ par $f(x)=7x-5$.
\begin{enumerate}
 \item Calculer\baremeque{2} $f(10)$, $f(\dfrac{3}{10})$ et $f(\sqrt{11})$.
 \item Donner\baremeque{2} le (ou les) antécédent(s) de 44 par $f$.
\end{enumerate}
\end{exercice}

\begin{exercice}
 Soit $f$\baremeexo{4} la fonction définie sur $\R$ par $f(x)=(x-5)^2-2$.
\begin{enumerate}
 \item Calculer\baremeque{2} $f(2)$ et $f(-3)$.
 \item Quelle\baremeque{1} est l'image de 5 par $f$ ?
 \item Donner\baremeque{1} un antécédent de $-2$ par $f$.
\end{enumerate}
\end{exercice}





\begin{exercice}
 Soit $h$\baremeexo{2} la fonction définie sur $\R$ par $h(x)=x^2$.
\begin{enumerate}
 \item Quels\baremeque{1} sont les antécédents de 7 ?
 \item Donner\baremeque{1} un réel qui n'a pas d'antécédent par $h$. Justifier.
\end{enumerate}
\end{exercice}

